{
  "version": 1,
  "maxPreviewsCached": 10,
  "vaults": [
    {
      "fsPath": "vault"
    }
  ],
  "useFMTitle": true,
  "useNoteTitleForLink": true,
  "noAutoCreateOnDefinition": true,
  "noLegacyNoteRef": true,
  "noXVaultWikiLink": true,
  "lookupConfirmVaultOnCreate": false,
  "mermaid": true,
  "useKatex": true,
  "autoFoldFrontmatter": true,
  "usePrettyRefs": true,
  "dev": {
    "previewV2Enabled": false
  },
  "lookup": {
    "note": {
      "selectionType": "selectionExtract",
      "leaveTrace": false
    }
  },
  "journal": {
    "dailyDomain": "daily",
    "name": "journal",
    "dateFormat": "y.MM.dd",
    "addBehavior": "childOfDomain",
    "firstDayOfWeek": 1
  },
  "scratch": {
    "name": "scratch",
    "dateFormat": "y.MM.dd.HHmmss",
    "addBehavior": "asOwnDomain"
  },
  "site": {
    "copyAssets": true,
    "siteHierarchies": [
      "travel.pictures"
    ],
    "siteRootDir": "docs",
    "siteUrl": "https://harshita-mindfire.github.io",
    "usePrettyRefs": true,
    "title": "Postcards",
    "description": "Joshi's Personal knowledge space",
    "usePrettyLinks": true,
    "siteNotesDir": "notes",
    "siteFaviconPath": "favicon.ico",
    "gh_edit_link": true,
    "gh_edit_link_text": "Edit this page on GitHub",
    "gh_edit_branch": "main",
    "gh_root": "docs/",
    "gh_edit_view_mode": "edit",
    "writeStubs": true,
    "siteIndex": "travel.pictures"
  },
  "initializeRemoteVaults": true,
  "seeds": {
    "dendron.templates": {}
  },
  "dendronVersion": "0.61.0"
}
